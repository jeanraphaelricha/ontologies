        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>isDisjointWith</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/distanceRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/distanceRelation">
