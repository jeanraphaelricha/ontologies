        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>isLeftOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/locationRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/locationRelation">
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:comment>A location relation describes a relation between a pair of locations e.g. &quot;Table1&quot; opposite &quot;Table2&quot;.   

A location relation can be either topological relation, directional relation or distance relation.

This relation intends to be use as an abstract relation for other location relations.   Hence, it may not be used directly.    Only sub-relations of this relation should be used</rdfs:comment>
        <rdfs:label>hasLocationRelation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/opposite -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/opposite">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>isOppositeOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/overlaps -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/overlaps">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>overlaps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/rightOf -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/rightOf">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/directionalRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>isRightOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/topologicalRelation -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/topologicalRelation">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/locationRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <vann:preferredNamespaceUri>An abstract relation for topological location relations</vann:preferredNamespaceUri>
        <rdfs:label>hasTopologicalRelation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/touches -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/touches">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>touches</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affectedBy -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affectedBy">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affects"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Visualisation"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Context"/>
        <rdfs:label>affectedBy</rdfs:label>
        <skos:definition>Relation that maps a Visualisation to a Context</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affects -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affects">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Context"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Visualisation"/>
        <rdfs:label>affects</rdfs:label>
        <skos:definition>Relation that maps a Context to a Visualisation choice</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atCoverageTime -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atCoverageTime">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:label>hasCoverageTime</rdfs:label>
        <skos:definition>Relation that maps a CoverageArea covered by a Sensor to a time:TemporalEntity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atLocationTime -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atLocationTime">
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:label>hasLocationTime</rdfs:label>
        <skos:definition>Relation that maps a Location of a Sensor to a time:TemporalEntity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atSensingTime -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atSensingTime">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#ObservationValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Denotes the sensing timestamp for a specific Observation Value</rdfs:comment>
        <rdfs:label>hasSensingTime</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centeredAround -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centeredAround">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centers"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#SensingLocation"/>
        <rdfs:label>isCenteredAround</rdfs:label>
        <skos:definition>Relation that maps a Sensor&apos;s Location to its CoverageArea.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centers -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centers">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#SensingLocation"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:label>centers</rdfs:label>
        <skos:definition>Relation that maps a CoverageArea to its Sensor&apos;s Location.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#composes -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#composes">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#isComposedOf"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/LocationMap"/>
        <rdfs:label>composes</rdfs:label>
        <skos:definition>The relation between the set of individual Locations that compose a LocationMap.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyCovers -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyCovers">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#isCurrentlyCoveredBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:comment>currentlyCovers is the object property that allows finding the Coverage Area of a Stationary or Mobile Sensor in time. The Coverage Area of a Stationary Sensor does not change in time, this is not the case for a Mobile Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Coverage Area.</rdfs:comment>
        <rdfs:label>currentlyCovers</rdfs:label>
        <skos:definition>currentlyCovers is the object property that allows finding the Coverage Area of a Stationary or Mobile Sensor in time. The Coverage Area of a Stationary Sensor does not vary in time, this is not the case for a Mobile Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Coverage Area.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyLocates -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyLocates">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#isCurrentlyLocatedAt"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Device"/>
        <rdfs:comment>The inverse of isCurrentlyLocatedAt</rdfs:comment>
        <rdfs:label>currentlyLocates</rdfs:label>
        <skos:definition>The inverse of isCurrentlyLocatedAt</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimitedBy -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimitedBy">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimits"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#SensingRange"/>
        <rdfs:label>isDelimitedBy</rdfs:label>
        <skos:definition>Defines the relation between CoverageArea and the sensor&apos;s SensingRange.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimits -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimits">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#SensingRange"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#CoverageArea"/>
        <rdfs:label>delimits</rdfs:label>
        <skos:definition>Defines the impact of the sensor&apos;s SensingRange on the geomerty of the CoverageArea.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#describes -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#describes">
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/LocationMap"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Infrastructure"/>
        <rdfs:label>describes</rdfs:label>
        <skos:definition>The relation between a LocationMap and an Infrastructure helps represent the spatial elements of the latter.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#describesLocation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#describesLocation">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#isDescribedByPoint"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#p"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#embeddedOn -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#embeddedOn">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasEmbeddedSystem"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#ExpansionCard"/>
        <rdfs:label>isEmbeddedOn</rdfs:label>
        <skos:definition>The relation helps discover the Systems deployed or embedded on a Device&apos;s ExpansionCard.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCapability -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCapability">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Service"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#Capability"/>
        <rdfs:label>hasCapability</rdfs:label>
        <skos:definition>This relation maps a Service to its Capability description.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCommunicationRange -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCommunicationRange">
