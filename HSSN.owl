        <vann:preferredNamespaceUri>An abstract relation for distance relations</vann:preferredNamespaceUri>
        <rdfs:label>hasDistanceRelation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/equals -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/equals">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/topologicalRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>equals</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/farFrom -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/farFrom">
        <rdfs:subPropertyOf rdf:resource="http://mssn.sigappfr.org/mssn/distanceRelation"/>
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/Location"/>
        <rdfs:label>isFarFrom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasMediaDescriptor -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasMediaDescriptor">
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/MultimediaData"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/MediaDescriptor"/>
        <rdfs:comment>A relation between a media segment and a media descriptor</rdfs:comment>
        <rdfs:label>hasMultimediaDescriptor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/hasMediaSegment -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/hasMediaSegment">
        <rdfs:domain rdf:resource="http://mssn.sigappfr.org/mssn/MediaDescriptor"/>
        <rdfs:range rdf:resource="http://mssn.sigappfr.org/mssn/MultimediaData"/>
        <rdfs:comment>A relation between MultimediaData and MediaSegment concept</rdfs:comment>
        <rdfs:label>hasMediaSegment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn/leftOf -->

    <owl:ObjectProperty rdf:about="http://mssn.sigappfr.org/mssn/leftOf">
